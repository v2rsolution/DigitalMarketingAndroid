<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="@dimen/dim_2">

    <ImageView
        android:layout_width="@dimen/dim_14"
        android:layout_height="@dimen/dim_14"
        android:layout_gravity="center_vertical"
        android:background="@drawable/bullet_border"
        android:padding="@dimen/dim_4"
        android:src="@drawable/bullet_center" />

    <com.wscubetech.seovideotutorials.custom_views.MyTextViewRegular
        android:id="@+id/textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="@dimen/dim_5"
        android:gravity="left"
        android:text=""
        android:textAlignment="inherit"
        android:textColor="#dd818181"
        android:textSize="@dimen/dim_14" />

</LinearLayout>

