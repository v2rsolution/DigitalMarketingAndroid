<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/activity_image_enlarge"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context="com.wscubetech.seovideotutorials.activities.ImageEnlargeActivity">

    <com.wscubetech.seovideotutorials.custom_views.TouchImageView
        android:id="@+id/img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:minHeight="@dimen/dim_180"
        android:layout_centerInParent="true"
        android:transitionName="img"/>

</RelativeLayout>
